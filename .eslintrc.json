{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "warn",
        "semi": ["warn", "always"],
        "no-param-reassign": "off",
        "import/extensions": "off"
    }
}
